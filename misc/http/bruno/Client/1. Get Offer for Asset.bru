meta {
  name: 1. Get Offer for Asset
  type: http
  seq: 4
}

get {
  url: {{consumer-api}}/automated/offer?providerUrl={{provider-dsp}}&assetId={{asset-id}}
  body: none
  auth: inherit
}

params:query {
  providerUrl: {{provider-dsp}}
  assetId: {{asset-id}}
}

vars:post-response {
  offer: res.body[0]
}

settings {
  encodeUrl: true
  timeout: 0
}
